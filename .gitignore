backend/build/
backend/.gradle/
backend/bin/
*.class
*.jar
*.war

# Node.js
middleware/node_modules/
middleware/dist/
middleware/logs/

# -----------------------------------------------------------------------------
# IDE
# -----------------------------------------------------------------------------
.idea/
*.iml
.vscode/
*.swp
*.swo
.project
.classpath
.settings/

# -----------------------------------------------------------------------------
# Environment & Secrets
# -----------------------------------------------------------------------------
.env
.env.local
.env.*.local
*.pem
*.key
!blockchain/config/keys/  # Demo keys are intentionally included

# -----------------------------------------------------------------------------
